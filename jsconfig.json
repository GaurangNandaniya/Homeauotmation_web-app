{
    "compileOnSave": true,
    "compilerOptions":{
      "module": "CommonJS",
      "target": "ES6",
      "baseUrl": ".",
      "paths": {
        "commonComponents": ["./src/commonComponents"],
        "utils/*": ["./src/utils/*"],
        "hooks": ["./src/hooks"],
        "RootPage/*":["./src/routes/*"],
        "constants/*":["./src/constants/*"],
        "contextAPI/*":["./src/contextAPI/*"],
        "HOCs":["./src/HOCs"],
        "RoomDetails/*":["./src/routes/routes/UserHomes/routes/HomeDetails/routes/Rooms/routes/RoomDetails/*"]
      }
    },
    "exclude": ["node_modules", "build","dist"],
    "include": ["./src/**/*"]
  }